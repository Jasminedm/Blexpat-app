<section class="container">
<div class="card-deck row">
  <% for(var i=0; i < plan.length; i++) { %>
    <div class="card col-sm-3">       
        <img src="<%= plan[i].postImg %>" class="card-img-top" style="width: 100%;height: auto;" alt="...">
        <div class="card-body">
          <h5 class="card-title"><%= plan[i].location %></h5>
        </div>
        <div class="card-footer">
          <a href="/showPlan/<%= plan[i]._id %>"><small class="text-muted">View More</small></a>
        </div>
    </div>
    <% } %>
  </section>

    
  




